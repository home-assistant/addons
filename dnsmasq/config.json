{
  "name": "Dnsmasq",
  "version": "0.5",
  "slug": "dnsmasq",
  "description": "A simple DNS server",
  "url": "https://home-assistant.io/addons/dnsmasq/",
  "startup": "system",
  "boot": "auto",
  "host_network": true,
  "options": {
    "defaults": ["8.8.8.8", "8.8.4.4"],
    "forwards": [],
    "hosts": [],
    "interface": "eth0"
  },
  "schema": {
    "defaults": ["str"],
    "forwards": [
      {
        "domain": "str",
        "server": "str"
      }
    ],
    "hosts": [
      {
        "host": "str",
        "ip": "str"
      }
    ],
    "interface": "str"
  },
  "image": "homeassistant/{arch}-addon-dnsmasq"
}
