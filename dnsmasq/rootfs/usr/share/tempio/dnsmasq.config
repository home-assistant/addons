# Automatically generated do not edit

no-resolv
no-hosts
keep-in-foreground
{{ if .log_queries }}
log-queries
{{ end }}
log-facility=-
no-poll
user=root
cache-size={{ .cache_size}}

# Default forward servers
{{ range .defaults }}
server={{ . }}
{{ end }}

# Domain forwards
{{ range .forwards }}
server=/{{ .domain }}/{{ .server }}
{{ end }}

# Static hosts
{{ range .hosts }}
address=/{{ .host }}/{{ .ip }}
{{ end }}

# Static srv-hosts
{{ range .services }}
srv-host={{ .srv }},{{ .host }},{{ .port }},{{ .priority }},{{ .weight }}
{{ end }}

# Static cname
{{ range .cnames }}
cname={{ .name }},{{ .target }}
{{ end }}
